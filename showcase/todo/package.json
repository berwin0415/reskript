    "antd": "^5.9.1",
    "core-js": "^3.32.2",
    "localforage": "^1.10.0",
    "nanoid": "^5.0.1",
    "ramda": "^0.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3"
  }
